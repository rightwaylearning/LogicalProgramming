package array_examples_test;

public class IsExistGivenElementInsideArray {

	public static void main(String[] args) {
		
		
	}
	
	
	boolean isElementPresent(Integer[] arr,Integer element) {
		
		if(arr != null) {
		
			for(int i = 0; i< arr.length; i++) {
				if(element == arr[i]) {
					return true;
				}
			}
			
		}
		
		
		return false;
	}
}
