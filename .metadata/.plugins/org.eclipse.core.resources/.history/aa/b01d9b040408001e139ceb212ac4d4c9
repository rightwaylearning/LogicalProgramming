package array_examples_test;

public class CountElementsOfArray {

	public static void main(String[] args) {
		
		Integer[] arr = {10,20,30,40,50};
		Integer elementCount =  counTElementOfArray(null);
	}
	

	 static Integer counTElementOfArray(Integer[] arr) {
		
		  if(arr != null) {
			  int count = 0;
			  for (int i = 0; i < arr.length; i++) {
				  count++;
			  }
			  return count;
		  }
		  return null;
	  }
	
}
